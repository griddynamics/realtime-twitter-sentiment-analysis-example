{
  "name": "Twitter-Stream-Classifier-Web-UI",
  "version": "1.0.0",
  "description": "Simple Web UI application for the set of sample apps which pull tweets related to some movies, drop the tweets to Kafka, classify the tweets as positive/negative and, finally, export counters and the tweets to Cassandra.",
  "scripts": {
    "start": "DEBUG=streaming-sample:* WEB_CLIENT_PORT=3005 node server/server.js",
    "lint": "eslint './*/**.js'",
    "postinstall": "npm run build",
    "clean": "rm -rf server/dist",
    "build": "npm run clean && cd server && NODE_ENV=production webpack",
    "precommit": "npm run lint"
  },
  "license": "Apache License 2.0",
  "dependencies": {
    "async": "^2.0.1",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.3.13",
    "cassandra-driver": "^3.1.1",
    "chart.js": "^2.3.0",
    "classnames": "^2.1.2",
    "cross-env": "^1.0.7",
    "css-loader": "^0.25.0",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.0",
    "jsdom": "^5.6.1",
    "md5": "^2.1.0",
    "moment": "^2.15.1",
    "node-libs-browser": "^0.5.2",
    "path": "^0.12.7",
    "postcss-loader": "^1.0.0",
    "raw-loader": "^0.5.1",
    "rc-calendar": "^7.1.0",
    "rc-time-picker": "^2.0.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-modal-dialog": "^4.0.2",
    "react-redux": "^4.2.1",
    "react-tooltip": "^3.2.1",
    "redux": "^3.2.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "resolve-url-loader": "^1.6.0",
    "socket.io": "^1.4.6",
    "store": "^1.3.20",
    "style-loader": "^0.12.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "svg-url-loader": "^1.1.0",
    "webpack": "^1.9.11"
  },
  "devDependencies": {
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "husky": "^0.11.9",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.9.1"
  }
}
